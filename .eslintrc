{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error"],
    "object-curly-spacing": ["error", "always"]
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
